<!doctype html>
<html lang="en">
  {{{include.head}}}
  <body data-controller="{{route.controller}}" data-action="{{route.action}}" data-view="{{route.view}}" data-renderer="{{route.renderer}}" data-rendered-view="{{route.renderedView}}" data-mode="{{config.citizen.mode}}">
    {{{include.header}}}
    <main>
      {{#each route.chain}}
        {{{output}}}
      {{/each}}
    </main>
    {{{include.footer}}}
  </body>
</html>
